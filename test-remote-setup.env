# EC2 Remote Setup Test Configuration
# Copy this to your EC2 instance and source it before running tests

# Database Configuration
export DATABASE_URL="your_neon_database_url_here"

# Test Settings
export NODE_ENV="test"
export TEST_MODE="ec2"

# Required for role seeding
export SETUP_VALIDATE_ONLY="false"

# Optional: Test with specific user
export TEST_ADMIN_EMAIL="admin@local.dev"
export TEST_ADMIN_PASSWORD="admin123"